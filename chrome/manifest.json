{
  "manifest_version": 2,
  "name": "Sync Saved Windows",
  "version": "2.6",
  "description": "Synchronise saved windows",
  "icons":
  {
    "48": "icons/icon.png",
    "96": "icons/icon.png"
  },

  "permissions": [
    "tabs",
    "storage",
    "identity",
    "*://www.googleapis.com/*",
    "*://accounts.google.com/*"
  ],

  "browser_action":
  {
    "browser_style": true,
    "default_icon": "icons/icon.png",
    "default_title": "Sync Saved Windows",
    "default_popup": "popup.html"
  },

  "background":
  {
    "scripts": ["background.js"]
  },

  "key": "",

  "oauth2": {
    "client_id": "155155797881-u7fso8pu3q8923ksipfp0c61u7a57uvj.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.appfolder",
      "email"
    ]
  }

}
